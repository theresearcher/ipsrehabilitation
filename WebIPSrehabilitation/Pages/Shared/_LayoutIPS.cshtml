                                                                <input type="password" class="form-control" placeholder="Repite la contraseña">
                                                            </div>
                                                            <div class="form-group text-center">
                                                                <button type="submit" class="btn btn-default ulockd-btn-styledark">Registrarme</button>
                                                            </div>
                                                        </form>
